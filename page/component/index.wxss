@import "../common/index.wxss";
.navigator-text{
margin:10rpx;
text-align:center;
border:1px dashed #ddd;
border-radius: 20rpx;
padding:15rpx;
color:cornflowerblue;
}
.index-picker{
  width: 40%;
  margin: 10rpx auto;
}
.moneyList{
  position:relative;
  background-color:#FFFFFF;
  line-height:1.41176471;
  font-size:34rpx;
  transition:.3s;
  padding:30rpx;
}
.moneyList .moneyItem{
  display: flex;
  padding:20rpx;
  border-bottom:1px dashed #ddd;
  font-size:24rpx;
  justify-content:space-between;

}
ec-canvas{
  width: 100%;
  height: 500rpx;
  margin-bottom: 40rpx;
}